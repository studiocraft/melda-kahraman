<!doctype html>
<html>
  <head>
  <link href='http://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

  <title>{{#resume.basics}}{{name}} - CV{{/resume.basics}}</title>

  <style>
  {{{css}}}
  </style>

  </head>
  <body>

  {{#resume.basics}}
  <header>
  <h1>{{name}}</h1>
  {{#if label}}
  <h2>{{label}}</h2>
  {{/if}}
  </header>

  <div id="content" class="container">
  <section id="basics">
    <div class="contact">
    <h3>Contact</h3>
    {{#if website}}
    <div class="website">
      <strong>Website</strong>
      {{website}}
    </div>
    {{/if}}
    {{#if email}}
    <div class="email">
      <strong>Email</strong>
      <a href="mailto:{{email}}">{{email}}</a>
    </div>
    {{/if}}
    {{#if phone}}
    <div class="phone">
      <strong>Phone</strong>
      {{phone}}
    </div>
    {{/if}}
    </div>
    {{#if summary}}
    <div class="summary">
      <h3>About</h3>
      <p>{{summary}}</p>
    </div>
    {{/if}}
    {{#if profiles.length}}
    <div id="profiles">
      {{#each profiles}}
      <div class="item">
        {{#if network}}
        <strong class="network">
          {{network}}
        </strong>
        {{/if}}
        {{#if url}}
        <div class="url">
          <a href="{{url}}">
          {{#if username}}
            {{username}}
          {{else}}
            {{url}}
          {{/if}}
          </a>
        </div>
        {{/if}}
      </div>
      {{/each}}
    </div>
    {{/if}}
  </section>
  {{/resume.basics}}

  {{#if resume.education.length}}
  <section id="education">
    <h2>Education</h2>
    {{#each resume.education}}
    <div class="item">

      {{#if institution}}
      <div class="institution">
        <h3>{{institution}}</h3>
      </div>
      {{/if}}

      {{#if area}}
      <div class="area">
        <h4>{{area}}</h4>
      </div>
      {{/if}}

      <div class="study_date">
        {{#if startDate}}
        <em class="startDate">
          {{startDate}}
        </em>
        {{/if}}
        {{#if endDate}}
        <em class="endDate">
          to {{endDate}}
        </em>
        {{else}}
        <em class="endDate">
          to Present
        </em>
        {{/if}}
      </div>

      {{#if gpa}}
      <div class="gpa">
        <span>Cumulative GPA {{gpa}}</span>
      </div>
      {{/if}}

      {{#if psyIndex}}
      <div class="psy_index">
        <span>Psychology Index {{psyIndex}}</span>
      </div>
      {{/if}}

      {{#if courses.length}}
      <ul class="courses">
        <li style="list-style:none;margin-left:-1em;margin-bottom:.5em;">Relevant Coursework</li>
        {{#each courses}}
        <li>{{.}}</li>
        {{/each}}
      </ul>
      {{/if}}

      {{#if dissertation}}
      <div class="dissertation">
        <span>Dissertation: </span>{{dissertation}}
      </div>
      {{/if}}
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.experience.length}}
  <section id="experience">
    <h2>Experience</h2>
    {{#each resume.experience}}
    <div class="item">
      {{#if company}}
      <h3 class="experience_name">
        {{company}}
      </h3>
      {{/if}}
      <ul class="inline">
      {{#if position}}
      <li class="experience_position">
        {{position}}
      </li>
      {{/if}}

      <li class="experience_date">
        {{#if startDate}}
        <em class="startDate">
          {{startDate}}
        </em>
        {{/if}}

        {{#if endDate}}
        <em class="endDate">
          to {{endDate}}
        </em>
        {{else}}
        <em class="endDate">
          to Present
        </em>
        {{/if}}
      </li>

      {{#if website}}
      <li class="experience_website">
        <a href="{{website}}">{{website}}</a>
      </li>
      {{/if}}
    </ul>
      {{#if lab}}
      <div class="experience_lab">
        <strong>{{lab}}</strong>
      </div>
      {{/if}}

      {{#if summary}}
      <div class="summary">
        <p>{{summary}}</p>
      </div>
      {{/if}}
      {{#if highlights.length}}
      <ul class="highlights">
        {{#each highlights}}
        <li>{{.}}</li>
        {{/each}}
      </ul>
      {{/if}}
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.volunteer.length}}
  <section id="volunteer">
    <h2>Volunteer</h2>
    {{#each resume.volunteer}}
    <div class="item">
      {{#if organization}}
      <h3 class="company">
        {{organization}}
      </h3>
      {{/if}}

      {{#if position}}
      <div class="volunteer_position">
        {{position}}
      </div>
      {{/if}}
      {{#if website}}

      <div class="volunteer_date">
        {{#if startDate}}
        <em class="startDate">
          {{startDate}}
        </em>
        {{/if}}
        {{#if endDate}}
        <em class="endDate">
          - {{endDate}}
        </em>
        {{else}}
        <em class="endDate">
          - Present
        </em>
        {{/if}}
      </div>

      <div class="work_website">
        <a href="{{website}}">{{website}}</a>
      </div>
      {{/if}}
      {{#if summary}}
      <div class="summary">
        <p>{{summary}}</p>
      </div>
      {{/if}}
      {{#if highlights.length}}
      <ul class="highlights">
        {{#each highlights}}
        <li>{{.}}</li>
        {{/each}}
      </ul>
      {{/if}}
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.skills.length}}
  <section id="skills">
    <h2>Skills</h2>
    {{#each resume.skills}}
    <div class="item">
      {{#if name}}
      <div class="name">
        {{name}}
      </div>
      {{/if}}
      {{#if level}}
      <div class="level">
        <em>{{level}}</em>
      </div>
      {{/if}}
      {{#if keywords.length}}
      <ul class="keywords">
        {{#each keywords}}
        <li>{{.}}</li>
        {{/each}}
      </ul>
      {{/if}}
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.languages.length}}
  <section id="languages">
    <h2>Languages</h2>
    {{#each resume.languages}}
    <div class="item">
      {{#if language}}
      <div class="language">
        {{language}}
      </div>
      {{/if}}
      {{#if fluency}}
      <div class="level">
        <em>{{fluency}}</em>
      </div>
      {{/if}}
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.awards.length}}
  <section id="awards">
    <h2>Honors</h2>
    {{#each resume.awards}}
    <div class="item">
      {{#if title}}
      <div class="awards">
        {{title}}
      </div>
      {{/if}}
      {{#if date}}
      <div class="date">
        <em>{{date}}</em>
      </div>
      {{/if}}
      {{#if awarder}}
      <div class="awarder">
        {{awarder}}
      </div>
      {{/if}}
      {{#if summary}}
      <div class="summary">
        {{summary}}
      </div>
      {{/if}}
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.membership.length}}
  <section id="membership">
    <h2>Membership</h2>
    {{#each resume.membership}}
    <div class="item">
      {{#if name}}
      <h3 class="membership_name">
        {{name}}
      </h3>
      {{/if}}

      <div class="membership_year">
        {{#if year}}
        <em class="year">
          {{year}} to Present
        </em>
        {{/if}}
      </div>
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.conferences.length}}
  <section id="conferences">
    <h2>Conferences</h2>
    {{#each resume.conferences}}
    <div class="item">
      {{#if name}}
      <h3 class="conference_name">
        {{name}}
      </h3>
      {{/if}}

      <div class="conference_year">
        {{#if year}}
        <em class="year">
          {{year}}
        </em>
        {{/if}}
        {{#if highlights.length}}
        <ul class="highlights">
          {{#each highlights}}
          <li>{{.}}</li>
          {{/each}}
        </ul>
        {{/if}}

      </div>
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.interests.length}}
  <section id="interests">
    <h2>Interests</h2>
    {{#each resume.interests}}
    <div class="item">
      {{#if name}}
      <div class="name">
        {{name}}
      </div>
      {{/if}}
      {{#if keywords.length}}
      <ul class="keywords">
        {{#each keywords}}
        <li>{{.}}</li>
        {{/each}}
      </ul>
      {{/if}}
    </div>
    {{/each}}
  </section>
  {{/if}}

  {{#if resume.references.length}}
  <section id="references">
    <h2>References</h2>
    {{#each resume.references}}
    <div class="item">
      {{#if reference}}
      <blockquote class="reference">
        {{reference}}
        {{#if name}}
        <footer> â€“
          <cite class="name">
            {{name}}
          </cite>
        </footer>
        {{/if}}
      </blockquote>
      {{/if}}
    </div>
    {{/each}}
    <p><small>Further references available on request.</small></p>
  </section>
  {{/if}}

  </div>
  </body>
</html>
